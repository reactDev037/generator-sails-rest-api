{
  "name": "generator-sails-rest-api",
  "version": "1.0.0-alpha.4",
  "description": "Yeoman generator that provides already configured and optimized Sails REST API with bundle of predefined features",
  "keywords": [
    "yeoman-generator",
    "sails",
    "boilerplate",
    "scaffolder",
    "rest",
